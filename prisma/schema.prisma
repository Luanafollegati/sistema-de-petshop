generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model animais {
  id      Int    @id @default(autoincrement())
  nome    String
  especie String
  idade   Int
  dono    String

  @@map("animais")
}
